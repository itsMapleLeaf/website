    "@itsmapleleaf/configs": "file:..\\configs",
    "astro": "^3.0.10",
    "eslint": "^8.48.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.17.1",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@itsmapleleaf/configs/eslint.config.cjs"
    ],
    "ignorePatterns": [
      "node_modules",
      "dist"
